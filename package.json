{
  "name": "hh-auto-apply",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "./src/app/index.ts",
  "scripts": {
    "start": "node --env-file=.env --import @bleed-believer/path-alias src/app/index.ts",
    "format": "prettier --write ./**/*.{ts,json}",
    "lint": "eslint ./src/**/*.ts --fix",
    "build": "tsc && tsc-alias"
