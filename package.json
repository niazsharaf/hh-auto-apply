  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "paths": {
      "@/": "./src/"
    }
  },
  "dependencies": {
    "@bleed-believer/path-alias": "^2.3.2",
    "dotenv": "^17.2.2",
    "module-alias": "^2.2.3",
    "prettier": "^3.6.2",
    "puppeteer-real-browser": "^1.4.4",
    "typescript": "^5.9.2"
  },
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4"
  }
}
